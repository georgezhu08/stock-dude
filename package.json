{
  "name": "convert-day-files",
  "version": "1.0.0",
  "description": "Convert .day stock files to JSON format",
  "main": "convert_day_to_json.ts",
  "scripts": {
    "getlist": "node --no-warnings --loader ts-node/esm src/get_stock_list_sina.ts",
    "convert": "node --no-warnings --loader ts-node/esm src/convert_tdx_to_json.ts",
    "select": "node --no-warnings --loader ts-node/esm src/select_stock.ts",
    "backtest": "node --no-warnings --loader ts-node/esm src/backtest.ts",
    "kline": "node --no-warnings --loader ts-node/esm src/generate_kline.ts",
    "all": "npm run getlist && npm run convert && npm run select && npm run backtest && npm run kline"
  },
  "author": "your_name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "canvas": "^3.1.0",
    "cli-progress": "^3.12.0",
    "echarts": "^5.6.0",
    "http": "^0.0.1-security",
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.9.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "type": "module"
}
